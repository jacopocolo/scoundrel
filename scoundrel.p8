pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
grdTiles = {16,17,18}
bkgrdTiles = {19,20,21,22}
p = {}
p.x = 8
p.y = 8
p.w = 4
p.h = 8
p.direction = 2
p.ox = 2
p.oy = 0
p.dx = 0
p.dy = 0
p.sprites={1,2,3}
p.sprite=p.sprites[1]--current sprite
p.state = "idle"
p.timer = 0

function dst(o1,o2)
 return sqrt((o1.x-o2.x)^2+(o1.y-o2.y)^2)
end

function mcheck(x,y)
 x=(x+p.ox)/8
 y=y/8
 w=(p.w)/8
 h=p.h/8
 if(x<0 or x+w>128) return false
 if(y<0 or y+h>64) return false
 for r=flr(y),flr(y+h-0.001) do
  for c=flr(x),flr(x+w-0.001) do
   if(fget(mget(c,r),0))then
    return false
   end
  end
 end
 return true
end

function pdraw()
  spr(p.sprite,p.x,p.y,1,1,p.direction==-1)
end

function changestate(s)
  p.state=s
  p.timer=0
end

function _draw()
 cls()
 mapdraw(0,0,0,0,128,16)
 pdraw()
end

function _update()
  b0=btn(0)
  b1=btn(1)
  b2=btn(5)

  if p.state=="idle" then
     p.sprite=p.sprites[1]
     if (b0 or b1) changestate("walk")
     if (b2) changestate("jump")
     if (mcheck(p.x,p.y+4)) changestate("fall")
  end

  if p.state=="walk" then
    if (b0) p.direction=-1
    if (b1) p.direction=1
    if (mcheck(p.x+p.direction,p.y)) then
      p.x+=p.direction
      p.sprite=p.sprites[(flr(p.timer/3)%3)+1]
    else p.x=flr(p.x)
    end
    if (not(b0 or b1)) changestate("idle")
    if (b2) changestate("jump")
    if (mcheck(p.x,p.y+4)) changestate("fall")
  end

  --falling
  if p.state=="fall" then
    p.sprite=p.sprites[3]
    if (mcheck(p.x,p.y+4)) then p.y=p.y+4
    else p.y=flr(p.y) changestate("idle")
    end
    if (mcheck(p.x+p.direction,p.y)) then
      if (b0) p.x-=1
      if (b1) p.x+=1
    end
  end

  --jumping
  if p.state=="jump" then
    p.sprite=p.sprites[3]
    if (b0) p.direction=-1
    if (b1) p.direction=1
    if (mcheck(p.x,p.y-4)) then p.y=p.y-4
    else p.y=flr(p.y) changestate("idle")
    end
    if (mcheck(p.x+p.direction*3,p.y)) then
      if (b0) p.x-=2
      if (b1) p.x+=2
    end
    if (not b2 or p.timer>7) changestate("idle")
  end

  --tile underneath the p with light effect
if mget(flr((p.x+4)/8),flr((p.y+8)/8))==grdTiles[2] then
  mset(flr((p.x+4)/8),flr((p.y+8)/8), grdTiles[3])
end

for x=0,15,1
  do
    for y=0,15,1
      do
        --reset floor tile if not under p
        if mget(x,y)==grdTiles[3] and (x~=flr((p.x+4)/8) or y~=flr((p.y+8)/8)) then
           mset(x,y,grdTiles[2])
        end
        light = 1.5--tiles
        tile = {}
        tile.x = flr(x*8)
        tile.y = flr(y*8)
        --darken everything
       if mget(x,y)==grdTiles[2] and dst(p, tile)>light*8 then
         mset(x,y,grdTiles[1])
       end
       if (mget(x,y)==bkgrdTiles[2] or mget(x,y)==bkgrdTiles[3] or mget(x,y)==bkgrdTiles[4]) and dst(p, tile)>light*8 then
         mset(x,y,bkgrdTiles[1])
       end
       --light up
       --floor
       if mget(x,y)==grdTiles[1] and dst(p,tile)<light*8 then
         mset(x,y,grdTiles[2])
       end
       --farther away background
       if (mget(x,y)==bkgrdTiles[1]) and (dst(p,tile)>light*8 and dst(p,tile)<(light*8)+8) then
         --flickering
          if (flr(time()) % 2 > 0 and flr(time()) % 2 < 4) then
            mset(x,y,bkgrdTiles[3]) else
              mset(x,y,bkgrdTiles[4])
          end
       end
       --background
       if (mget(x,y)==bkgrdTiles[2] or mget(x,y)==bkgrdTiles[3] or mget(x,y)==bkgrdTiles[4])  and dst(p,tile)<light*8 then
         mset(x,y,bkgrdTiles[2])
       end
      end
  end

  p.timer+=1
end

